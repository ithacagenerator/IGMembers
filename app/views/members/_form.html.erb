<%= semantic_form_for @member do |f| %>
  <%= f.inputs do %>
    <%= f.input :lname %>
    <%= f.input :fname %>
    <%= f.input :address %>
    <%= f.input :city %>
    <%= f.input :state %>
    <%= f.input :zip %>
    <%= f.input :email %>
    <%= f.input :phone %>
  <% end %>

  <%= f.actions do %>
    <%= f.action :submit, :as => :input %>
  <% end %>
<% end %>
