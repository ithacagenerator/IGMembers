<li>
  <%= membership.membership_type.name %>
  Member
  <% if membership.end.nil? %>
    since 
    <%= membership.start.to_s %>
  <% else %>
    from <%= membership.start.to_s %>
    until <%= membership.end.to_s %>
  <% end %>

  <% if !membership.complete? %>
  <%= membership.incomplete_checklist_items.count %> incomplete checklist items.
  <% end %>

  <% if !current_user.nil? && current_user.admin? %>
    <%= link_to "Edit", edit_membership_path(membership) %>
  <% end %>
  <br />
</li>
