<% provide(:title, 'Add Membership') %>

<h1>Change Membership for <%= @membership.member.name %></h1>

<div class="row">
  <div class="span6 offset3">
    <%= semantic_form_for @membership do |f| %>
      <%= render 'form', f: f %>
      <%= f.submit "Save Changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
